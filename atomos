<script>
const queryString = window.location.search;
const urlParams = new URLSearchParams(queryString);
const d = urlParams.get('d');
const e = urlParams.get('e');
let job;

function factory() {
  if (typeof(job) == "undefined") {
    job = new Worker("d.js?e=d");
  }
   job.onmessage = function(event) {
      switch (e) {
        case 'i':  
          stopWorker();
          return window.location.replace("/beforus/?done=")+`Great I took it. I wrapped it around another plant to be.`;
          break;
        default:
          job = undefined;
          const pass = document.createElement("p");
          pass.textContent = `Ahh, I see you didn't ask for anything.`;
          divInfo.appendChild(pass);
          break;
    }
  };
}
function stopWorker() {
  job.terminate();
  job = undefined;
}
window.onload = () => {
  factory();
};
</script>
